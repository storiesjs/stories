{
  "name": "@stories-js/vue",
  "sideEffects": false,
  "version": "0.1.5",
  "license": "GPL-2.0",
  "author": "StoriesJS team",
  "description": "Vue 3 Stories renderer and wrapper for custom elements of Stories to be used as first-class Vue 3 components",
  "keywords": [
    "stories",
    "storiesjs",
    "angular",
    "angularjs",
    "react",
    "reactjs",
    "vue",
    "vuejs",
    "components",
    "web components",
    "documentation",
    "jamstack",
    "markup",
    "mdx",
    "frontend",
    "backend",
    "reuseable",
    "tool",
    "ui",
    "user interface"
  ],
  "main": "./dist/vue.common.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist/*", 
    "types/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storiesjs/stories.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vue src/index.ts",
    "lint": "vue-cli-service lint",
    "clean": "rimraf dist",
    "test": "echo stories-vue2 testing",
    "build.web-types": "node ./scripts/build-web-types.js",
    "build.vetur": "node ./scripts/build-vetur.js",
    "copy": "node ./scripts/copy-css.js",
    "sync": "sh ./scripts/sync.sh",
    "pub": "npm config set //registry.npmjs.org/:_authToken=${NPM_TOKEN} && yarn publish --access public"
  },
  "dependencies": {
    "@stories-js/components": "0.1.5"
  },
  "devDependencies": {
    "@babel/types": "^7.17.0",
    "@stencil/core": "^2.14.2",
    "@vue/test-utils": "^2.0.0-rc.16",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "change-case": "^4.1.2",
    "fs-extra": "^10.0.1",
    "vue": "^3.0.0"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "web-types": "dist/web-types.json"
}
