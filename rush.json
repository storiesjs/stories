{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.56.0",
  "pnpmVersion": "6.7.1",
  "pnpmOptions": {
    "strictPeerDependencies": true,
    "useWorkspaces": true
  },

  "nodeSupportedVersionRange": ">=12.13.0 <13.0.0 || >=14.15.0 <15.0.0",
  "ensureConsistentVersions": true,

  "gitPolicy": {
  },

  "repository": {
    "url": "https://github.com/storiesjs/stories",
    "defaultBranch": "main"
  },

  "eventHooks": {
    "preRushInstall": [
    ],

    "postRushInstall": [],

    "preRushBuild": [],

    "postRushBuild": []
  },

  "variants": [
  ],

  "projects": [
    {
      "packageName": "@stories/api",
      "projectFolder": "lib/api",
      "reviewCategory": "tools"
    },
    {
      "packageName": "@stories/story-viewer",
      "projectFolder": "lib/story-viewer",
      "reviewCategory": "production",
      "shouldPublish": true,
      "publishFolder": "temp/publish"
    }
  ]
}
