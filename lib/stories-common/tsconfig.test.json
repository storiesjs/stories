{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "types": ["node", "jest", "dom"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "declaration": true,
    "strict": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true
  },
  "include": [
    "src"
  ]
}


