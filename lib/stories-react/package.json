{
  "name": "@stories/stories-react",
  "version": "0.1.0",
  "description": "Stories React is an open source adapter between Stories Viewer and React based application",
  "keywords": [],
  "repository": "git@github.com:storiesjs/storiesjs.git",
  "license": "MIT",
  "author": "akserg <akserg@gmail.com>",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn run clean",
    "build": "node build.js && yarn run build:types && yarn build:css",
    "build:publish": "yarn build",
    "build:types": "tsc -p tsconfig.json",
    "build:css": "postcss src/index.css -o dist/index.css -u autoprefixer -u cssnano",
    "start": "tsc -p tsconfig.json --watch"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@code-hike/classer": "^0.0.0-aa6efee",
    "@stories/stories-viewer": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@types/fs-extra": "^5.0.4",
    "@types/glob": "^5.0.35",
    "@types/node": "^9.4.6",
    "@types/react": "^17.0.2",
    "autoprefixer": "^10.2.4",
    "cross-env": "^5.1.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "rimraf": "^2.6.1",
    "typescript": "4.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17",
    "react-dom": "^16.8.0 || ^17"
  }
}
