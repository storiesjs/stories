{
  "name": "@stories/stories-react",
  "sideEffects": false,
  "version": "0.1.0",
  "license": "GPL-2.0",
  "private": true,
  "description": "React specific wrapper for component-library",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "build.watch": "yarn run clean && yarn run compile --watch",
    "clean": "rimraf dist",
    "compile": "yarn run tsc",
    "tsc": "tsc -p .",
    "rollup": "rollup -c"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/node": "^15.12.2",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@testing-library/jest-dom": "5.16.1",
    "jest": "^23.0.0",
    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@stories/stories-common": "0.1.0",
    "@stories/stories-ui": "0.1.0"
  },
  "jest": {
    "preset": "ts-jest",
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  }
}
