  "files": [
    "dist",
    "esm",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@componentdriven/csf": "0.0.2-alpha.0"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "core-js": "^3.7.0",
    "cross-env": "^5.0.1",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-remove-sourcemaps": "^1.0.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "4.0.3"
  }
